<%= form_for([@assignment, @submission]) do |f| %>
  <h2>
    <%= @submission.student["name"] %>
  </h2>
  <p>
    <%= f.label :html_url %><br>
    <%= f.text_area :html_url %>
  </p>
  <p>
    <%= f.label :repo_url %><br>
    <%= f.text_area :repo_url %>
  </p>
  <p>
    <%= f.label :status %><br>
    <%= f.text_area :status %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
